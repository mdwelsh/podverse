{
  "compilerOptions": {
    "strict": true,
    "noEmitOnError": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "jsxImportSource": "ai-jsx",
    "esModuleInterop": true,
    "outDir": "dist"
  },
  "include": ["src"]
}
